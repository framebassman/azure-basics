{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppKeyVault": {
    "Endpoint": "https://reporting13bjqz1801fzkoi.vault.azure.net/"
  },
  "Serilog": {
    "LevelSwitches": { "controlSwitch": "Verbose" },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "TicketStore.Api": "Verbose"
      }
    },
    "Enrich": ["WithThreadId", "FromLogContext", "WithMachineName", "WithEnvironmentName"],
    "Properties": {
      "Application": "ProjectTasks.Api"
    },
    "WriteTo": [
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "MinimumLevel": "Verbose",
            "WriteTo": [
              {
                "Name": "Console",
                "Args": {
                  "outputTemplate": "[{Timestamp:o} {Level:u3}] ({Application}/{MachineName}/{ThreadId}) {Message}{NewLine}{Exception}",
                  "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                  "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter,Serilog.Formatting.Compact"
                }
              }
            ]
          },
          "restrictedToMinimumLevel": "Verbose"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args":
        {
          "nodeUris": "https://kolenka-inc-4135333449.eu-central-1.bonsaisearch.net",
          "indexFormat": "filebeat-7.10.2-{0:yyyy.MM.dd}",
          "minimumLogEventLevel": "Information",
          "connectionGlobalHeaders": "Authorization=Bearer Zm10dWNlcmMwbTpnMXpiZmJqOGx5",
          "templateName": "projectTasksTemplate",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv7"
        }
      }
    ]
  }
}
